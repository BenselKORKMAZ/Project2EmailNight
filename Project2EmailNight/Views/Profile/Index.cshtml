@model Project2EmailNight.Dtos.UserEditDto

@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br /><br />
    <form method="post" enctype="multipart/form-data">
        <input type="text" asp-for="Name" class="form-control" placeholder="Adiniz" />
        <br />
        <input type="text" asp-for="Surname" class="form-control" placeholder="Soyadiniz" />
        <br />
        <input type="text" asp-for="Email" class="form-control" placeholder="Email Adresi" />
        <br />
        <input type="file" asp-for="Image" class="form-control" placeholder="Gorsel Url" />
        <br />
        <input type="text" asp-for="Password" class="form-control" placeholder="Sifreniz" />
        <br />
        <button class="btn btn-info">Bilgileri Kaydet</button>
    </form>
</div>
